  <!-- UV TEXT  EMIT -->
   <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>face_emit</object-name>
  </effect>
  
    <animation>
    <type>select</type>
    <object-name>face_emit</object-name>
	<condition>
             <equals>
		<property>/environment/atmosphere/altitude-half-to-sun</property>
                <value>0</value>
             </equals>
	</condition>
    </animation> 

    <animation>
    <type>material</type>
    <object-name>face_emit</object-name>
       <condition>
	   <and>
             <equals>
		<property>/environment/atmosphere/altitude-half-to-sun</property>
                <value>0</value>
             </equals>
	      <equals>
	        <property>/sim/model/lights/FluorLamp[0]</property>
	        <value>true</value>
	      </equals>
	      <equals>
	        <property>/sim/model/lights/FluorLamp[1]</property>
	        <value>true</value>
	      </equals>
	   </and>
       </condition>
       <emission>
	   <red>0.2</red>
	   <green>0.25</green>
	   <blue>0.7035</blue>
       </emission>
   </animation>

   <animation>
    <type>material</type>
    <object-name>face_emit</object-name>
       <condition>
             <equals>
		<property>/environment/atmosphere/altitude-half-to-sun</property>
                <value>0</value>
             </equals>
	   <or>
	      <equals>
	        <property>/sim/model/lights/FluorLamp[0]</property>
	        <value>true</value>
	      </equals>
	      <equals>
	        <property>/sim/model/lights/FluorLamp[1]</property>
	        <value>true</value>
	      </equals>
	   </or>
       </condition>
       <emission>
		 <red>0.10</red>
		 <green>0.125</green>
		 <blue>0.3417</blue>
       </emission>
   </animation>

