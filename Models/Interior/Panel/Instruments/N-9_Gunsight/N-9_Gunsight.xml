<PropertyList>

  <path>N-9_Gunsight.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>ball</object-name>
    <object-name>TurnBase</object-name>
    <object-name>Turn</object-name>
    <object-name>Tlens.Frame1</object-name>
    <object-name>Tlens.Frame2</object-name>
    <object-name>Tlens.Frame3</object-name>
    <object-name>Knob1</object-name>
    <object-name>LensSwitch</object-name>
    <object-name>visses</object-name>

    <object-name>Reticul</object-name>
    <object-name>vitre_turn</object-name>
    <object-name>Dlens</object-name>
    <object-name>Plens</object-name>
    <object-name>Tlens</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre_turn</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/A-26-Invader/Models/Effects/glass/glass_shader.png</texture>
    <object-name>vitre-turn</object-name>
    <object-name>Dlens</object-name>
    <object-name>Plens</object-name>
    <object-name>Tlens</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>ball</object-name>
    <property>instrumentation/slip-skid-ball/indicated-slip-skid</property>
    <min> -1.1 </min>
    <max>  1.1 </max>
    <interpolation>
      <entry><ind> -1.1 </ind><dep> -6.2 </dep></entry>
      <entry><ind>  1.1 </ind><dep>  6.2 </dep></entry>
    </interpolation>
    <center>
      <x-m> 0.00 </x-m>
      <y-m> 0.00 </y-m>
      <z-m> 0.22 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
